<div class="row">
    <h1>Actions</h1>

    <table>
        <tr>
        <th>Type</th>
        <th>Price</th>
        <th>Where</th>
        </tr>
        {{#each actions}}
        <tr>
            <td>{{type}}</td>
            <td>{{cost}}</td>
            <td>{{timestamp}}</td>
        </tr>
        {{/each}}
    </table>
</div>